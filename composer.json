{
    "name": "project/default",
    "description": "Projeto Padrão - Micro Framework para aplicações PHP",
    "minimum-stability": "stable",
    "authors": [{
        "name": "Jackson Douglas B. Feijão",
        "email": "jkdouglas21@gmail.com",
        "role": "Developer"
    }],
    "config": {
        "vendor-dir": "vendor"
    },
    "autoload": {
        "psr-4": {
            "Library\\":"lib/library/",
            "App\\":"app/",
            "Services\\": "services/source/"
        },
        "classmap": [
            "app/apiservices",
            "app/control/dashboard",
            "app/control/tests",
            "app/control",
            "app/model"
        ],
        "files": [
            "init.php",
            "lib/library/Support/Helpers.php",
            "services/boot/Helpers.php"
        ]
    },    
    "require": {
        "filp/whoops": "^2.7",
        "twig/twig": "^3.0"
    }
}
